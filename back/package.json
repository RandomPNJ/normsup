    "grunt-nodemon": "~0.4.2",
    "grunt-shell": "~2.1.0",
    "grunt-ts": "~6.0.0-beta.19",
    "grunt-tslint": "~5.0.2",
    "load-grunt-tasks": "~4.0.0",
    "tslint": "~5.10.0",
    "mocha": "~5.2.0",
