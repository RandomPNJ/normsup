{
  "name": "stack-back",
  "version": "1.0.0",
  "private": true,
  "main": "server/index.js",
  "engines": {
    "node": "8.12.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "install-blockchain-ibm-fr": "mkdir -p node_modules/@blockchain-ibm-fr/ && cp ./resources/dependencies/blockchain-ibm* node_modules/@blockchain-ibm-fr && cd node_modules/@blockchain-ibm-fr/ && tar -zxvf blockchain-ibm-fr-blocknjser-access-*.tgz && mv package blocknjser-access && tar -zxvf blockchain-ibm-fr-blocknjser-common-*.tgz && mv package blocknjser-common && rm *.tgz",
    "docker-install-blockchain-ibm-fr": "set-x ; ls -al ./resources/dependencies/ ; mkdir -p node_modules/@blockchain-ibm-fr/ && cp ./resources/dependencies/blockchain-ibm* node_modules/@blockchain-ibm-fr && cd node_modules/@blockchain-ibm-fr/ && tar -zxvf blockchain-ibm-fr-blocknjser-access-*.tgz && mv package blocknjser-access && tar -zxvf blockchain-ibm-fr-blocknjser-common-*.tgz && mv package blocknjser-common && rm *.tgz ; set +x",
    "docker-install-blockchain-ibm-fr-common": "mkdir -p node_modules/@blockchain-ibm-fr/ && cp ./resources/dependencies/blockchain-ibm* node_modules/@blockchain-ibm-fr && cd node_modules/@blockchain-ibm-fr/ && tar -zxvf blockchain-ibm-fr-blocknjser-common-*.tgz && mv package blocknjser-common && rm *.tgz",
    "start": "node server/index.js",
    "dev": "nodemon",
    "test": "cross-env NODE_ENV=test mocha -r  ts-node/register ./server/mocha.conf.ts ./server/mocha.global.ts ./server/**/*.spec.ts --recursive",
    "test-jenkins": "JUNIT_REPORT_PATH=/report.xml mocha -r  ts-node/register ./server/mocha.conf.ts ./server/mocha.global.ts ./server/**/*.spec.ts --recursive --colors --reporter mocha-jenkins-reporter",
    "prod": "node dist/server/index.js",
    "prebuild": "npm run clean",
    "deploy": "nodemon dist/index.js",
    "postbuild": "cp -Rp package.json package-lock.json dist",
    "build": "tsc",
    "lint": "eslint ./server ",
    "lint:fix": "eslint ./server --fix",
    "clean": "rimraf coverage dist .nyc_output"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "~1.17.0",
    "@types/chai-things": "0.0.33",
    "@types/compression": "0.0.36",
    "@types/express": "~4.11.1",
    "@types/helmet": "0.0.42",
