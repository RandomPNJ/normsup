    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "reflect-metadata": "~0.1.12",
    "request": "^2.79.0",
    "request-promise": "^4.2.1",
    "routing-controllers": "^0.7.7",
    "sleep": "~5.2.3",
    "typedi": "~0.7.3",
    "typescript": "~2.8.3",
    "typings": "^2.1.1",
    "unirest": "^0.5.1",
    "uuid": "~3.3.2",
    "winston": "~2.4.0",
    "winston-daily-rotate-file": "~1.7.2"
  },
  "devDependencies": {
    "@types/chai": "~4.1.3",
    "@types/chai-as-promised": "~7.1.0",
