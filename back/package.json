{
  "name": "stack-back",
  "version": "1.0.0",
  "private": true,
  "main": "server/index.js",
  "engines": {
    "node": "4.4.5",
    "npm": "3.9.5"
  },
  "scripts": {
    "start": "node server/index.js",
    "predev": "npm run lint & npm run clean",
    "dev": "nodemon --watch server server/index.js --exec babel-node",
    "test": "cross-env NODE_ENV=test nyc mocha ./mocha.conf.js mocha.global.js ./server --recursive",
    "prod": "node dist/server/index.js",
    "prebuild": "npm run clean",
    "postbuild": "cp -Rp manifests/* package.json npm-shrinkwrap.json dist",
    "build": "babel server/ -d dist/server",
    "lint": "eslint ./server ",
    "lint:fix": "eslint ./server --fix",
    "clean": "rimraf coverage dist .nyc_output"
  },
  "dependencies": {
    "amqplib": "~0.5.1",
    "babel-polyfill": "~6.26.0",
    "bluebird": "~3.5.1",
    "body-parser": "~1.18.2",
    "cloudant": "~1.7.1",
    "compression": "~1.6.2",
    "cross-env": "~5.1.1",
    "express": "~4.16.2",
    "helmet": "~3.6.1",
    "joi": "~12.0.0",
    "lodash": "~4.17.4",
    "method-override": "~2.3.7",
    "moment": "~2.17.0",
    "pg-promise": "~6.5.4",
    "redis": "~2.8.0",
    "request": "~2.79.0",
    "request-promise": "~4.2.1",
    "uuid": "~3.1.0",
    "winston": "~2.4.0",
    "winston-daily-rotate-file": "~1.4.2"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.2",
    "babel-plugin-istanbul": "~4.1.5",
    "babel-preset-env": "~1.6.0",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.26.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~7.1.1",
    "chai-http": "~3.0.0",
    "chai-things": "~0.2.0",
    "dirty-chai": "~1.2.0",
    "eslint": "~4.10.0",
    "mocha": "~4.0.1",
    "nodemon": "~1.12.1",
    "nyc": "~11.3.0",
    "rimraf": "~2.6.2",
    "sinon": "~4.1.2",
    "sinon-chai": "~2.14.0"
  }
}
